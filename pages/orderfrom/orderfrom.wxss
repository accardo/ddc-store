/* pages/orderfrom/orderfrom.wxss */
.img-box{
  width: 100%;
  height: auto;
  padding: 20rpx 30rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #ccc;
  overflow: hidden;
}
.img-box .img-txt{
  font-size: 30rpx;

}
.img-list{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  /*justify-content: space-between;*/
  align-items: center;
}
.img-list image{
  width: 31%;
  height: 180rpx;
  margin-top: 30rpx;
  margin-left: 0;
  margin-right: 15rpx;
  border-radius: 10rpx;
}
.img-list image:last-child{
  margin-right: 0;
}