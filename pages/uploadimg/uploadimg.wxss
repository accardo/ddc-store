/* pages/uploadimg/uploadimg.wxss */
@import '../../common/common';
page,.upload{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.products{
  padding-bottom: 150rpx;
  box-sizing: border-box;
}
.upload-box{
  margin: 0 auto;
  width: 240rpx;
  height: 230rpx;
  border: 1px dashed #ccc;
  padding:15rpx;
  margin-top: 50rpx;
  text-align: center;
  box-sizing: border-box;
}
.upload-box text{
  font-size: 32rpx;
}
.upload-box image{
  display: block;
  width: 150rpx;
  margin: 0 auto;
  height: 150rpx;
}
.img-list{
  width: 100%;
  height: auto;
  display: flex;
  padding:0 30rpx 30rpx;
  box-sizing: border-box;
  align-items:center;
  justify-content: space-between;
  flex-wrap: wrap;
}
.img-list image{
  margin-top:30rpx;
  width: 31%;
  height: 190rpx;
  border-radius: 10rpx;
}